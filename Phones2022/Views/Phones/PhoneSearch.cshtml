@model IEnumerable<Phones2022.Models.Phone>

<div id="searchresults">
    <p>
        Found <b> @Model.Count() </b> items
    </p>
    <ul>
        @foreach (var item in Model)
        {
            <li>
                @Html.ActionLink(item.Name, "Details", new { id = item.Id })
                @Html.DisplayFor(modelItem => item.Price)
            </li>
        }

    </ul>

</div>